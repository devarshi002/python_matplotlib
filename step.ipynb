   "source": [
    "import matplotlib.pyplot as plt\n",
    "\n",
    "x = [1,2,3,4,5]\n",
    "y = [2,3,4,5,6]\n",
    "\n",
    "plt.step(x,y, color=\"g\", marker=\"o\", mfc=\"black\", label=\"python\")\n",
    "plt.title(\"python\")\n",
    "plt.xlabel(\"x - axis\")\n",
    "plt.ylabel(\"y - axis\")\n",
    "plt.grid()\n",
    "plt.legend()\n",
    "plt.show()"
